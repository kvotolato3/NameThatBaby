<div class='wrapper'>
  <%= form_tag(email_lookup_path, method: "get") do %>
    <%= label_tag(:email, "Please enter email address:") %>
    <%= text_field_tag :email, nil, options = {autofocus: true} %>
    <%= submit_tag("Next") %>
  <% end %>
  <%= link_to 'Sign Up', new_user_registration_path %>
</div>
